{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"allowJs": true,
		"declaration": true,
		"baseUrl": "./",
		"esModuleInterop": true,
		"module": "commonjs",
		"noImplicitAny": true,
		"noImplicitReturns": false,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"outDir": ".build",
		"rootDir": "./",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"target": "es2020"
	}
}